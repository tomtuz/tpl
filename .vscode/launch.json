{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug: tpl <repository>",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/src/main.py",
            "args": ["repository"],
            "env": {
                "PYTHONPATH": "${workspaceFolder}/.venv/bin/python3",
            },
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "justMyCode": false
        }
    ]
}
